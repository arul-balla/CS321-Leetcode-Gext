{
    "name": "LeetCode Extension",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "tabs",
      "activeTab", 
      "scripting",
      "notifications"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["http://*.nytimes.com/*"],
        "js": ["leetcode-content-script.js"]
      }
    ],
    "action": {
        "default_popup": "popup.html"
      }
    
  }